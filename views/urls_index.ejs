<!DOCTYPE html>
<html lang="en">
<head>
  <% include partials/_header %>
</head>
<body>
  <table>
    <% for (var key in urls) { %>
      <tr>
        <td><a href="/u/<%= key %>"><%= key %></a> : <%= urls[key]%></td>
        <td>
        <a href="/urls/<%= key %>">Edit</a>
        </td>
        <td>
          <form method="POST" action="/urls/<%= key %>/delete">
          <button type="submit" value="submit">Delete</button>
        </td>
      </tr>
    <% } %>
    </table>
  </table>
</body>
</html>